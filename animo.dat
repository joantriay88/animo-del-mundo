set terminal png size 1300,400
datey="`date +%d/%m/%y\t%H%M --date='1 days ago'`"
daten="`date +%d/%m/%y\t%H%M`"
set key left top
set grid
set title "Ánimo instantáneo del mundo"
set xlabel "Día/Hora UTC"
set timefmt "%d/%m/%y\t%H%M"
set yrange [ 0 :  ]
set xdata time
set xrange [datey: ]
set format x "%d/%m\n%H:%M"
set ylabel "tweets por minuto"
set timefmt "%s"
set output  "/home/Dropbox/Public/all_tpm.png"
plot './plots/all_tpm.txt' using 1:2 with lines title 'Amor' linecolor rgb "pink", \
'./plots/all_tpm.txt' using 1:3 with lines title 'Alegría' linecolor rgb "yellow", \
'./plots/all_tpm.txt' using 1:4 with lines title 'Sorpresa' linecolor rgb "orange", \
'./plots/all_tpm.txt' using 1:5 with lines title 'Ira' linecolor rgb "red", \
'./plots/all_tpm.txt' using 1:6 with lines title 'Envidia' linecolor rgb "green", \
'./plots/all_tpm.txt' using 1:7 with lines title 'Tristeza' linecolor rgb "blue", \
'./plots/all_tpm.txt' using 1:8 with lines title 'Miedo' linecolor rgb "black"
